{
  "version": "1.0.0",
  "sep": 24,
  "vectors": [
    {
      "id": "sep24-deposit-interactive-success",
      "description": "Create interactive deposit transaction",
      "type": "deposit_interactive",
      "input": {
        "account": "GACLIENTACCOUNT0000000000000000000000000000000000000000000",
        "asset_code": "USDC",
        "amount": "100.00"
      },
      "expected": {
        "status": 200,
        "response_fields": ["id", "type", "url"],
        "transaction_status": "incomplete"
      }
    },
    {
      "id": "sep24-withdraw-interactive-success",
      "description": "Create interactive withdraw transaction",
      "type": "withdraw_interactive",
      "input": {
        "account": "GACLIENTACCOUNT0000000000000000000000000000000000000000000",
        "asset_code": "USDC",
        "amount": "50.00"
      },
      "expected": {
        "status": 200,
        "response_fields": ["id", "type", "url"],
        "transaction_status": "incomplete"
      }
    },
    {
      "id": "sep24-unauthorized",
      "description": "Reject protected endpoint without bearer token",
      "type": "auth_failure",
      "input": {
        "endpoint": "/transactions/deposit/interactive"
      },
      "expected": {
        "status": 403,
        "error": "missing bearer token"
      }
    },
    {
      "id": "sep24-transaction-by-stellar-id-not-found",
      "description": "Return 404 when a transaction cannot be found by stellar_transaction_id",
      "type": "transaction_lookup_failure",
      "input": {
        "endpoint": "/transaction",
        "query": {
          "stellar_transaction_id": "021581089cb614be94b0ac5dc71cadf23a1cd96a2584152268de505ee2e5e999"
        }
      },
      "expected": {
        "status": 404,
        "error": "transaction not found"
      }
    },
    {
      "id": "sep24-transactions-filter-kind-withdrawal",
      "description": "Filter transactions by kind=withdrawal",
      "type": "transactions_filter",
      "input": {
        "endpoint": "/transactions",
        "query": {
          "asset_code": "USDC",
          "kind": "withdrawal"
        }
      },
      "expected": {
        "status": 200
      }
    },
    {
      "id": "sep24-invalid-transition",
      "description": "Reject direct transition from incomplete to completed",
      "type": "state_machine_failure",
      "input": {
        "from": "incomplete",
        "to": "completed"
      },
      "expected": {
        "valid": false,
        "error_code": "INVALID_TRANSITION"
      }
    },
    {
      "id": "sep24-fee-success",
      "description": "Return fee estimate for operation",
      "type": "fee_lookup",
      "input": {
        "operation": "deposit",
        "asset_code": "USDC",
        "amount": "100.00"
      },
      "expected": {
        "status": 200,
        "response_fields": ["fee"]
      }
    }
  ]
}
