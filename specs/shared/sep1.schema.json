{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://sep-reference.local/specs/shared/sep1.schema.json",
  "title": "SEP-1 stellar.toml",
  "type": "object",
  "required": [
    "VERSION",
    "SIGNING_KEY",
    "NETWORK_PASSPHRASE",
    "WEB_AUTH_ENDPOINT"
  ],
  "properties": {
    "VERSION": {
      "type": "string"
    },
    "SIGNING_KEY": {
      "type": "string",
      "pattern": "^G[A-Z2-7]{55}$"
    },
    "NETWORK_PASSPHRASE": {
      "type": "string"
    },
    "WEB_AUTH_ENDPOINT": {
      "type": "string",
      "format": "uri"
    },
    "TRANSFER_SERVER": {
      "type": "string",
      "format": "uri"
    },
    "TRANSFER_SERVER_SEP0024": {
      "type": "string",
      "format": "uri"
    }
  },
  "additionalProperties": true
}
